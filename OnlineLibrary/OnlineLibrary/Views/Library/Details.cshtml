@model Book
@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div class="container">
    <h1>@Model.Name</h1>
    <table>
        <tr>
            <td>Author:  @Model.Author</td>

            
        </tr>
        <tr>
            <td>Pages:  @Model.Pages</td>
        </tr>
        <tr>
            <td>Isbn:  @Model.Isbn</td>
        </tr>
        <tr>
            <td>Description:  @Model.Description</td>
        </tr>
    </table>

</div>